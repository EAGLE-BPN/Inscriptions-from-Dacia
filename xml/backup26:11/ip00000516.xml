    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>Leg</abbr><ex>ionis</ex></expan> XIII <expan><abbr>Ge</abbr><ex>minae</ex></expan> <lb n="2"/><expan><abbr>Aur</abbr><ex>elius, -elii</ex></expan> Cal<expan><ex>l</ex><abbr>istr</abbr><ex>atus</ex></expan>, -ati).
                        
                        
                    </ab>

                
                
