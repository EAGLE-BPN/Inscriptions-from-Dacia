                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/>Valeria Gemina <lb n="3"/><expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>n<am>n</am></abbr><ex>is</ex></expan> <lb n="4"/>XVII <expan><abbr>d</abbr><ex>iebus</ex></expan> X <lb n="5"/><expan><abbr>h</abbr><ex>ic</ex></expan> <expan><abbr>s</abbr><ex>ita</ex></expan> <expan><abbr>e</abbr><ex>st</ex></expan> <lb n="6"/><expan><abbr>P</abbr><ex>ublius</ex></expan> <expan><abbr>Ael</abbr><ex>ius</ex></expan> Papi <lb n="7"/>rianus (centurio) <lb n="8"/><expan><abbr>coh</abbr><ex>ortis</ex></expan> III <expan><abbr>Cam</abbr><ex>pestris</ex></expan> <lb n="9"/>coniugi <lb n="10"/><expan><abbr>b</abbr><ex>ene</ex></expan> <expan><abbr>m</abbr><ex>erenti</ex></expan> <lb n="11"/>posuit.
                        
                        
