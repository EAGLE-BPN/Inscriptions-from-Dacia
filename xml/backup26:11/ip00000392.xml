    <ab>
                        <lb n="1"/>
                        
                        
                            
                            Aelia Ia<supplied reason="lost">nuaria<certainty locus="name" match="preceding-sibling::text()" cert="low"/><expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>ann</abbr><ex>is</ex></expan></supplied> <lb n="2"/>XIII <expan><abbr>m</abbr><ex>ensibus</ex></expan> III <supplied reason="lost" cert="low">et Aelia Aga </supplied> <lb n="3"/>the <expan><abbr>vix</abbr><ex>it</ex></expan> [<expan><abbr>n<am>n</am></abbr><ex>is</ex></expan> - - - <lb n="4"/>- - - - - -].
                        
                        
                    </ab>

                
                
