                            <expan><abbr>Imp</abbr><ex>eratori</ex></expan> <expan><abbr>Caes</abbr><ex>ari</ex></expan> <expan><abbr>L</abbr><ex>ucio</ex></expan> Aelio <lb n="2"/><expan><abbr>Aurel</abbr><ex>io</ex></expan> Commodo <lb n="3"/><expan><abbr>p</abbr><ex>io</ex></expan> <expan><abbr>f</abbr><ex>elici</ex></expan> <expan><abbr>Aug</abbr><ex>usto</ex></expan> <expan><abbr>Sarm</abbr><ex>atico</ex></expan> Ger<lb break="no" n="4"/><expan><abbr>manic</abbr><ex>o</ex></expan> <expan><abbr>max</abbr><ex>imo</ex></expan> <expan><abbr>Britt</abbr><ex>anico</ex></expan> <lb n="5"/>ob honorem <expan><abbr>dec</abbr><ex>urionatus</ex></expan> <lb n="6"/><expan><abbr>Ael</abbr><ex>ii</ex></expan> Constantis <lb n="7"/><expan><abbr>proc</abbr><ex>uratoris</ex></expan> eius et Ị<expan><abbr>uli</abbr><ex>i</ex></expan> Pa<lb break="no" n="8"/><supplied reason="lost">c</supplied>ạtiani quondam <lb n="9"/><supplied reason="lost"><expan><abbr>praef</abbr><ex>ecti</ex></expan> <expan><abbr>T</abbr><ex>itus</ex></expan> F</supplied><expan><abbr>l</abbr><ex>avius</ex></expan> Ianua<lb break="no" n="10"/><supplied reason="lost" cert="low">rius pat </supplied><expan><abbr>r</abbr><ex>onus</ex></expan> <expan><abbr>col</abbr><ex>oniae</ex></expan> et <lb n="11"/><expan><abbr>T</abbr><ex>itus</ex></expan> <expan><abbr>Fl</abbr><ex>avius</ex></expan> Germanus <lb n="12"/><expan><abbr>dec</abbr><ex>urio</ex></expan> <expan><abbr>col</abbr><ex>oniae</ex></expan> quod <expan><abbr>dec</abbr><ex>uriones</ex></expan> <lb n="13"/>alares promise<lb break="no" n="14"/>runt pecunia su<lb break="no" n="15"/>a posuerunt<supplied reason="lost"><expan><abbr>l</abbr><ex>oco</ex></expan></supplied> <expan><abbr>d</abbr><ex>ato</ex></expan> <lb n="16"/><expan><abbr>d</abbr><ex>ecurionum</ex></expan> <expan><abbr>d</abbr><ex>ecreto</ex></expan>.
                        
                        
