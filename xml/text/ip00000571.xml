                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/><expan><abbr>G</abbr><ex>aius</ex></expan> <expan><abbr>Iul</abbr><ex>ius</ex></expan> Ve <lb n="3"/>recun <lb n="4"/>do <expan><abbr>vet</abbr><ex>erano</ex></expan> ex <expan><abbr>dec</abbr><ex>urione</ex></expan> <expan><abbr>al</abbr><ex>ae</ex></expan> <lb n="5"/><expan><abbr>Cl</abbr><ex>audiae</ex></expan> <expan><abbr>Cl</abbr><ex>audius</ex></expan> Lon <lb n="6"/>ginus ami <lb n="7"/>cus et he <lb n="8"/>res <expan><abbr>p</abbr><ex>ecunia</ex></expan> <expan><abbr>s</abbr><ex>ua</ex></expan>.
                        
                        
