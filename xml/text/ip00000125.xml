                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/>Aurelius <lb n="3"/>Brisanus <lb n="4"/>vixit annis <lb n="5"/>L <expan><abbr>Aur</abbr><ex>elius</ex></expan> Mucatius <lb n="6"/>ed Aurelius <lb n="7"/>Maximianus <lb n="8"/><expan><abbr>fili</abbr><ex>i</ex></expan> ed heredes <lb n="9"/><expan><abbr>p</abbr><ex>atri</ex></expan> <expan><abbr>p</abbr><ex>ientissimo</ex></expan> <expan><abbr>p</abbr><ex>osuerunt</ex></expan>.
                        
                        
