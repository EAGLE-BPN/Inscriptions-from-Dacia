                            <expan><abbr>Mar</abbr><ex cert="low">ius</ex></expan> <expan><abbr>Aurel</abbr><ex cert="low">ianus</ex></expan> <lb n="2"/>princeps <expan><abbr>n</abbr><ex>umeri</ex></expan> <lb n="3"/>port<supplied reason="lost">i</supplied>cum d <lb n="4"/>eum <expan><abbr>stra</abbr><ex>vit</ex></expan>.
                        
                        
