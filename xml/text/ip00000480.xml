                            Invi<supplied reason="lost">cto</supplied> <lb n="2"/>deo pro <lb n="3"/>salute <expan><abbr>C</abbr><ex>ai</ex></expan> Iu<supplied reason="lost"><expan><abbr>l</abbr><ex>ii</ex></expan></supplied> <lb n="4"/>Rufini l<supplied reason="lost">ibe</supplied> <lb n="5"/>rorumqu[e <lb n="6"/>eiu]s Spatalu[s <lb n="7"/>se]<expan><abbr>r</abbr><ex>vus</ex></expan> actor | <supplied reason="lost"><expan><abbr>v</abbr><ex>otum</ex></expan></supplied> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>l</abbr><ex>aetus</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
