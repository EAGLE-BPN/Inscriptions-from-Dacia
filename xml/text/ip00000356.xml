                            <expan><abbr>Imp</abbr><ex>erator</ex></expan> <expan><abbr>Caes</abbr><ex>ar</ex></expan> <expan><abbr>M</abbr><ex>arcus</ex></expan> <expan><abbr>Aur</abbr><ex>elius</ex></expan> Antoni <lb n="2"/>nus pius <expan><abbr>Aug</abbr><ex>ustus</ex></expan> felix Part<expan><ex>h</ex><abbr>i</abbr></expan> <lb n="3"/>cus maximus pontifex <lb n="4"/><expan><abbr>max</abbr><ex>imus</ex></expan> <expan><abbr>Brit</abbr><ex>annicus</ex></expan> <expan><abbr>max</abbr><ex>imus</ex></expan> <expan><abbr>trib</abbr><ex>uniciae</ex></expan> <expan><abbr>pot</abbr><ex>estatis</ex></expan> <lb n="5"/>5 XVI <expan><abbr>imp</abbr><ex>erator</ex></expan> II co<expan><ex>n</ex><abbr>s</abbr><ex>ul</ex></expan> IIII <expan><abbr>p</abbr><ex>ater</ex></expan> <expan><abbr>p</abbr><ex>atriae</ex></expan> pro <lb n="6"/>co<expan><ex>n</ex><abbr>s</abbr><ex>ul</ex></expan> fecit.
                        
                        
