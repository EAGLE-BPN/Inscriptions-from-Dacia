    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> </supplied><gap reason="lost" quantity="3" unit="character"/> <lb n="2"/><expan><abbr>C</abbr><ex>aius</ex></expan> Candi <lb n="3"/>dius Pa <lb n="4"/>truinus <lb n="5"/><expan><abbr>prae</abbr><ex>fectus</ex></expan> <expan><abbr>coh</abbr><ex>ortis</ex></expan> <lb n="6"/><supplied reason="lost">I</supplied> <expan><abbr>Ael</abbr><ex>iae</ex></expan> Gaeṣ(atorum) <lb n="7"/><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"> </supplied><gap reason="lost" quantity="3" unit="character"/>.
                        
                        
