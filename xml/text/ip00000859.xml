    <ab>
                        <lb n="1"/>
                        
                        
                            
                            [- - - - - - <lb n="2"/>- - - <expan><abbr>coh</abbr><ex>ors</ex></expan> II <lb n="3"/>H]ịspa[<expan><abbr>nor</abbr><ex>um</ex></expan> - - - <lb n="4"/>n]um[ini maiesta <lb n="5"/>tique eius].
                        
                        
                    </ab>

                
                
