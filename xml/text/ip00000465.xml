                            <supplied reason="lost">D</supplied>eo invicto <lb n="2"/><expan><abbr><supplied reason="lost">Mi</supplied>t</abbr><ex>h</ex>rae</expan> <expan><abbr>sac</abbr><ex>rum</ex></expan> <lb n="3"/><supplied reason="lost">.</supplied> Statorius <lb n="4"/><supplied reason="lost">....</supplied>anus <expan><abbr>dec</abbr><ex>urio</ex></expan> <lb n="5"/>5 <supplied reason="lost">et</supplied> flamen m <lb n="6"/><expan><abbr><supplied reason="lost">uni</supplied>c</abbr><ex>ipii</ex></expan> <expan><abbr>Sep</abbr><ex>timii</ex></expan> Ap<supplied reason="lost"><expan><abbr>ul</abbr><ex>ensis</ex></expan></supplied> <lb n="7"/><supplied reason="lost">in</supplied>victi templum pr<supplied reason="lost">o</supplied> <lb n="8"/><supplied reason="lost">sal</supplied>utem sua suorum <lb n="9"/><supplied reason="lost">que p</supplied>ecunia mea feci.
                        
                        
