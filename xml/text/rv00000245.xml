                            <supplied reason="lost"><expan><abbr>Imp</abbr><ex>erator</ex></expan> <expan><abbr>Caes</abbr><ex>ar</ex></expan> Di</supplied>vi Hadriani <expan><abbr>fil</abbr><ex>ius</ex></expan> <supplied reason="lost">divi</supplied> <lb n="2"/><supplied reason="lost">Traiani P</supplied>arthici nepo<supplied reason="lost">s divi</supplied> <lb n="3"/><supplied reason="lost">Nervae p</supplied>ronepos <expan><abbr>T</abbr><ex>itus</ex></expan> Aẹ<supplied reason="lost">lius</supplied> <lb n="4"/><supplied reason="lost">Hadrianus A</supplied>ntoninus <expan><abbr>Aug</abbr><ex>ustus</ex></expan> <supplied reason="lost">pius</supplied> <lb n="5"/><supplied reason="lost">tribunicia</supplied> <expan><abbr>potest</abbr><ex>ate</ex></expan> VI co<expan><ex>n</ex><abbr>s</abbr><ex>ul</ex></expan>Ị<supplied reason="lost">II <expan><abbr>p</abbr><ex>ater</ex></expan> <expan><abbr>p</abbr><ex>atriae</ex></expan></supplied> <lb n="6"/><supplied reason="lost">fecit per al</supplied>ạm II Pannoni<supplied reason="lost">orum</supplied>
                        
                        
                    </ab>

                
                
