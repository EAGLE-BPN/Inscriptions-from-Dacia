                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/>Aelia Sura <lb n="3"/><expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>an</abbr><ex>nis</ex></expan> <lb n="4"/>XXX et <lb n="5"/>Aelius <lb n="6"/>Saturni<lb break="no" n="7"/>nus <expan><abbr>vix</abbr><ex>it</ex></expan> <lb n="8"/><expan><abbr>an</abbr><ex>nis</ex></expan> III et <lb n="9"/><expan><abbr>Ael</abbr><ex>ius</ex></expan> Vale<lb break="no" n="10"/>ntinus <lb n="11"/><expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>an</abbr><ex>nis</ex></expan> <lb n="12"/>III Aelius <lb n="13"/>Siro <expan><abbr>fi</abbr><ex>liis</ex></expan> <lb n="14"/>et <expan><abbr>coniu</abbr><ex>gi</ex></expan> <lb n="15"/>pientis<lb break="no" n="16"/>simis.
                        
                        
                    </ab>

                
                
