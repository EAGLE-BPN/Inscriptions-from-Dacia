                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>o</abbr><ex>ptimo</ex></expan> <expan><abbr>m</abbr><ex>aximo</ex></expan> <lb n="2"/>Aurelius <lb n="3"/>Valentinus <lb n="4"/><supplied reason="lost" cert="low"> - - - </supplied> <lb n="5"/><expan><abbr>refec</abbr><ex>it</ex></expan> <expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <supplied reason="lost"> <expan><abbr>m</abbr><ex>erito</ex></expan></supplied>.
                        
                        
