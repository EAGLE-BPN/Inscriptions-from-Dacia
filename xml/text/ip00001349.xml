    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>Leg</abbr><ex>ionis</ex></expan> II <expan><abbr>Ad</abbr><ex>iutricis</ex></expan> <lb n="2"/><expan><abbr>p</abbr><ex>iae</ex></expan> <expan><abbr>f</abbr><ex>idelis</ex></expan>.
                        
                        
