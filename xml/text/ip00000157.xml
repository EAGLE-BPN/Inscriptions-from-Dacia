    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>Gen</abbr><ex>io</ex></expan> scho <supplied reason="lost"> <expan><abbr>l</abbr><ex>ae</ex></expan></supplied> <lb n="2"/><expan><abbr>b</abbr><ex>ene</ex></expan>&lt;<expan><abbr>f</abbr><ex>iciariorum</ex></expan>&gt; sub Oc <supplied reason="lost"> <expan><abbr>t</abbr><ex>avio</ex></expan></supplied> <lb n="3"/>Iu<supplied reason="omitted">l</supplied>ian <supplied reason="lost"> o</supplied> le&lt;<expan><abbr>g</abbr><ex>ato</ex></expan>&gt; <lb n="4"/><expan><abbr>Aug</abbr><ex>usti</ex></expan> <supplied reason="lost"> <expan><abbr>pr</abbr><ex>o</ex></expan></supplied> <supplied reason="omitted">p</supplied><expan><abbr>r</abbr><ex>aetore</ex></expan> AB <lb n="5"/>APMIHAAN <lb n="6"/>Iu&lt;<expan><abbr>l</abbr><ex>io</ex></expan>&gt; Val <supplied reason="lost"> e</supplied>n <supplied reason="lost"> te</supplied> <lb n="7"/>- - - - - -.
                        
                        
