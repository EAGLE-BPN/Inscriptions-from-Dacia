    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>O</abbr><ex>ptimo</ex></expan> <expan><abbr>M</abbr><ex>aximo</ex></expan> <lb n="2"/>Dulcen <lb n="3"/>o <expan><abbr>P</abbr><ex>ublius</ex></expan> Gaius <lb n="4"/>Valerianu <lb n="5"/>s (centurio) <expan><abbr>leg</abbr><ex>ionis</ex></expan> X Fre <lb n="6"/>tensis re <lb n="7"/><expan><abbr>g</abbr><ex>ionarius</ex></expan> <expan><abbr>r</abbr><ex>egionis</ex></expan> Neridon- <lb n="8"/><expan><abbr>is</abbr><ex>- - -</ex></expan> <expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
