    <ab>
                        <lb n="1"/>
                        
                        
                            
                            Deo Libero <lb n="2"/>patri Titus <lb n="3"/>Flavius <lb n="4"/>Valentianus <lb n="5"/>5 b<expan><ex>ene</ex><abbr>f</abbr><ex>iciarius</ex></expan> co<expan><ex>n</ex><abbr>s</abbr><ex>ularis</ex></expan>.
                        
                        
