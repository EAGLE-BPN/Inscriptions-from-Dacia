    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>S</abbr><ex>oli</ex></expan> <expan><abbr>I</abbr><ex>nvicto</ex></expan> <expan><abbr>M</abbr><ex>ithrae</ex></expan> <expan><abbr>Cornel</abbr><ex>ius</ex></expan> Cornelianus ex <expan><abbr>v</abbr><ex>oto</ex></expan> <expan><abbr>p</abbr><ex>osuit</ex></expan>.
                        
                        
