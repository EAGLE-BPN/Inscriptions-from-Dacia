    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>o</abbr><ex>ptimo</ex></expan> <expan><abbr>m</abbr><ex>aximo</ex></expan> <lb n="2"/>et diis de <lb n="3"/><expan><abbr>abusq</abbr><ex>ue</ex></expan> et <lb n="4"/>genio loci <lb n="5"/>Optatus <lb n="6"/><expan><abbr>praef</abbr><ex>ectus</ex></expan> <expan><abbr>leg</abbr><ex>ionis</ex></expan> <lb n="7"/>V <expan><abbr>M</abbr><ex>acedonicae</ex></expan> <expan><abbr>c</abbr><ex>onstantis</ex></expan> <expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
