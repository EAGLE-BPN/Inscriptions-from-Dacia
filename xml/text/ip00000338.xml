                        <lb n="1"/>
                        
                        
                            
                            Silvano <lb n="2"/>domest<expan><ex>i</ex><abbr>c</abbr><ex>o</ex></expan> <lb n="3"/>sacrum <lb n="4"/><expan><abbr>Val</abbr><ex>erius</ex></expan> <expan><abbr>Them</abbr><ex>o</ex></expan> ou The<expan><ex>i</ex><abbr>m</abbr><ex>es</ex></expan> <lb n="5"/>5 v<supplied reason="lost"><expan><abbr>et</abbr><ex>eranus</ex></expan></supplied> et <expan><abbr>dec</abbr><ex>urio</ex></expan> <lb n="6"/><expan><abbr>municip</abbr><ex>ii</ex></expan> <lb n="7"/><supplied reason="lost"><expan><abbr>v</abbr><ex>otum</ex></expan></supplied> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
