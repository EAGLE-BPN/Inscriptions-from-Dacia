                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/><expan><abbr>T</abbr><ex>iberius</ex></expan> <expan><abbr>Clau</abbr><ex>dius</ex></expan> <lb n="3"/><expan><abbr>Ti</abbr><ex>berii</ex></expan> filius <lb n="4"/>Quirina Pris<lb break="no" n="5"/>cus Hemesa <lb n="6"/>ex <expan><abbr>c</abbr><ex>ustode</ex></expan> <expan><abbr>a</abbr><ex>rmorum</ex></expan> vixit <lb n="7"/>annis L, <expan><abbr>mil</abbr><ex>itavit</ex></expan> <lb n="8"/><expan><abbr>n<am>n</am></abbr><ex>is</ex></expan> XXVI <lb n="9"/>post <expan><abbr>missio</abbr><ex>nem</ex></expan> <lb n="10"/>vixit <expan><abbr>n<am>n</am></abbr><ex>os</ex></expan> II <lb n="11"/>convete<lb break="no" n="12"/>rani qui et <lb n="13"/>heredes <lb n="14"/>b<expan><ex>ene</ex><abbr>m</abbr><ex>erenti</ex></expan> <expan><abbr>p</abbr><ex>osuerunt</ex></expan>
                        
                        
