                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>O</abbr><ex>ptimo</ex></expan> <expan><abbr>M</abbr><ex>aximo</ex></expan> <lb n="2"/>Iunoni Regi<lb break="no" n="3"/>nae et pro sa<lb break="no" n="4"/>lute <expan><abbr>Imp</abbr><ex>eratoris</ex></expan> <expan><abbr>L</abbr><ex>ucii</ex></expan> Sep<lb break="no" n="5"/><expan><abbr>timi</abbr><ex>i</ex></expan> Severi <expan><abbr>Aug</abbr><ex>usti</ex></expan> <lb n="6"/>et <expan><abbr>L</abbr><ex>ucii</ex></expan> <expan><abbr>Sep</abbr><ex>timii</ex></expan> Anto<lb break="no" n="7"/><del rend="erasure">nini et Getae <expan><abbr>Caes</abbr><ex>aris</ex></expan></del> <lb n="8"/>posuit <expan><abbr>Cl</abbr><ex>audius</ex></expan> <lb n="9"/><expan><abbr>Coc</abbr><ex>ceius</ex></expan> de suo <lb n="10"/>X <expan><abbr>K</abbr><ex>alendas</ex></expan> <expan><abbr>Iul</abbr><ex>ias</ex></expan> <expan><abbr>Vic</abbr><ex>torino</ex></expan> et Se<lb break="no" n="11"/>vero co<expan><ex>n</ex><abbr>s</abbr><ex>ulibus</ex></expan>
                        
                        
