    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <supplied reason="lost">G</supplied>enio <expan><abbr>n</abbr><ex>umeri</ex></expan> P<supplied reason="lost"><expan><abbr>al</abbr><ex>myrenorum</ex></expan></supplied> <expan><abbr>Tib</abbr><ex>iscensium</ex></expan> <lb n="2"/><supplied reason="lost">et</supplied> <expan><abbr>hor</abbr><ex>rei</ex></expan> et dis <expan><abbr>p<supplied reason="lost">a</supplied>t</abbr><ex>riis</ex></expan> et pro <lb n="3"/><supplied reason="lost">sal</supplied>ute <expan><abbr>Aug<am>g</am></abbr><ex>ustorum</ex></expan> <expan><abbr><supplied reason="lost">n</supplied>n</abbr><ex>ostrorum</ex></expan> <expan><abbr>P</abbr><ex>ublius</ex></expan> <expan><abbr>Ael</abbr><ex>ius</ex></expan> <lb n="4"/><supplied reason="lost" cert="low">Ser</supplied>vius <expan><abbr>vet</abbr><ex>eranus</ex></expan> <supplied reason="lost">e</supplied>x <expan><abbr>opt</abbr><ex>ione</ex></expan> <lb n="5"/><supplied reason="lost" cert="low">cum sui</supplied>s ex vot<supplied reason="lost">o pos</supplied>uit.
                        
                        
