    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/>Cassia Sura <expan><abbr>vix</abbr><ex>it</ex></expan> <lb n="3"/><expan><abbr>an</abbr><ex>nis</ex></expan> XL <expan><abbr>Fl</abbr><ex>avius</ex></expan> Iulian <lb n="4"/>us <expan><abbr>fil</abbr><ex>ius</ex></expan> <expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>an</abbr><ex>nis</ex></expan> V <lb n="5"/>5 <expan><abbr>Fl</abbr><ex>avius</ex></expan> Moesicus <lb n="6"/>coniugi et <expan><abbr>fil</abbr><ex>io</ex></expan> <lb n="7"/><expan><abbr>piis</abbr><ex>simis</ex></expan> posuit <lb n="8"/><expan><abbr>b</abbr><ex>ene</ex></expan> <expan><abbr>m</abbr><ex>erentibus</ex></expan>.
                        
                        
                    </ab>

                
                
