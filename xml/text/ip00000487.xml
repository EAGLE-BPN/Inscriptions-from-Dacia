                            [- - - - - - <lb n="2"/>pro salute <lb n="3"/>......]<expan><abbr>tili</abbr><ex>i</ex></expan> Ru <lb n="4"/><supplied reason="lost">fi</supplied>ni <expan><abbr>cond</abbr><ex>uctoris</ex></expan> <lb n="5"/><supplied reason="lost">s</supplied>alinarum <lb n="6"/>5 Ursio <expan><abbr>ser</abbr><ex>vus</ex></expan> <lb n="7"/><expan><abbr>act</abbr><ex>or</ex></expan> verna.
                        
                        
                    </ab>

                
                
