    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/><expan><abbr>Flav</abbr><ex>ius</ex></expan> Qui <lb n="3"/>ntus <expan><abbr>pri</abbr><ex>nceps</ex></expan> <lb n="4"/>ex (numero) Ma<expan><ex>u</ex><abbr>r</abbr><ex>orum</ex></expan> <lb n="5"/><expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>an</abbr><ex>nis</ex></expan> XXX <lb n="6"/>Iulia Max <lb n="7"/>ima <expan><abbr>con</abbr><ex>iugi</ex></expan> <lb n="8"/><expan><abbr>p</abbr><ex>ientissimo</ex></expan> <expan><abbr>f</abbr><ex>aciendum</ex></expan> <expan><abbr>c</abbr><ex>uravit</ex></expan>.
                        
                        
