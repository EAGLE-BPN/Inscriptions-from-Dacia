                            <supplied reason="lost"><expan><abbr>C</abbr><ex>ohortis</ex></expan> I His</supplied><expan><abbr>p</abbr><ex>anorum</ex></expan> (quingenariae) <expan><abbr>T</abbr><ex>iti</ex></expan> <expan><abbr>P</abbr><ex>- - -</ex></expan> <expan><abbr>Lib</abbr><ex cert="low">eralis</ex></expan> (centuria)<certainty match="preceding-sibling::node()" locus="value"/>
                        
                        
