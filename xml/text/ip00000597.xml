                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>o</abbr><ex>ptimo</ex></expan> <expan><abbr>m</abbr><ex>aximo</ex></expan> <expan><abbr>D</abbr><ex>olicheno</ex></expan> <lb n="2"/>Iulius Valentin<supplied reason="lost">us</supplied> <lb n="3"/>flamen <expan><abbr>m</abbr><ex>unicipii</ex></expan> <lb n="4"/>pro salutem <lb n="5"/>suam suorum <lb n="6"/>que omnium <lb n="7"/><expan><abbr>v</abbr><ex>oto</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan> <expan><abbr>p</abbr><ex>osuit</ex></expan>.
                        
                        
