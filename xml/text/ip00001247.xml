    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>Imp</abbr><ex>erator</ex></expan> Cae<supplied reason="lost"><expan><abbr>s</abbr><ex>ar</ex></expan> <expan><abbr>M</abbr><ex>arcus</ex></expan> <expan><abbr>Aurel</abbr><ex>ius</ex></expan> Severus</supplied> <lb n="2"/><del rend="erasure">Alexan[der</del> Pius Felix Augustus] <lb n="3"/>balnea<supplied reason="lost">s <expan><abbr>coh</abbr><ex>ortis</ex></expan> II <expan><abbr>Fl</abbr><ex>aviae</ex></expan> <expan><abbr>Commagenor</abbr><ex>um</ex></expan></supplied> <lb n="4"/>Severia<supplied reason="lost">nae <expan><abbr>vetust</abbr><ex>ate</ex></expan> dilapsas res</supplied> <lb n="5"/>tituit s<supplied reason="lost">ub</supplied><gap reason="lost" quantity="3" unit="character"/><supplied reason="lost"><expan><abbr>co</abbr><ex>n</ex><abbr>s</abbr><ex>ulari</ex></expan></supplied> <lb n="6"/><expan><abbr>Dac</abbr><ex>iarum</ex></expan> III c<supplied reason="lost">urante </supplied><gap reason="lost" quantity="3" unit="character"/> <lb n="7"/>diano, p<supplied reason="lost"><expan><abbr>raef</abbr><ex>ecto</ex></expan> <expan><abbr>coh</abbr><ex>ortis</ex></expan> II <expan><abbr>Fl</abbr><ex>aviae</ex></expan> <expan><abbr>Com</abbr><ex>magenorum</ex></expan> Severi</supplied> <lb n="8"/>anae <supplied reason="lost">Alexandrianae</supplied>.
                        
                        
