    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/><expan><abbr>Aurel</abbr><ex>ii</ex></expan> Masinni <lb n="3"/><expan><abbr>dupl</abbr><ex>arii</ex></expan> al<expan><ex>a</ex><abbr>e</abbr></expan> (miliariae) <lb n="4"/>vixit annis <lb n="5"/>5 XXXVI Iulia Se <lb n="6"/>vera coniux <lb n="7"/>et <expan><abbr>Fl</abbr><ex>avius</ex></expan> Saturni <lb n="8"/>nus heres <lb n="9"/>ob merita posue <lb n="10"/>10 runt <expan><abbr>f</abbr><ex>aciendum</ex></expan> <expan><abbr>c</abbr><ex>uraverunt</ex></expan>.
                        
                        
