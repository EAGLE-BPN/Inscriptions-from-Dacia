                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/><expan><abbr>Ant</abbr><ex>onio</ex></expan> Floro b<expan><ex>ene</ex><abbr>f</abbr><ex>iciario</ex></expan> <lb n="3"/>co<expan><ex>n</ex><abbr>s</abbr><ex>ularis</ex></expan> <expan><abbr>mil</abbr><ex>itavit</ex></expan> <expan><abbr>n<am>n</am></abbr><ex>is</ex></expan> XXIII <lb n="4"/>vixit <expan><abbr>n<am>n</am></abbr><ex>is</ex></expan> XL. <expan><abbr>Ant</abbr><ex>onius</ex></expan> <lb n="5"/>Florianus op<lb break="no" n="6"/>tio <expan><abbr>leg</abbr><ex>ionis</ex></expan> I <expan><abbr>Ital</abbr><ex>icae</ex></expan> fra<lb break="no" n="7"/>ter posuit.
                        
                        
