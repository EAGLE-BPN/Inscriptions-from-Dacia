                            <expan><abbr>D</abbr><ex>is</ex></expan> <expan><abbr>M</abbr><ex>anibus</ex></expan> <lb n="2"/>Victoriae <lb n="3"/>Afr<expan><ex>a</ex><abbr>e</abbr></expan> <expan><abbr>vix</abbr><ex>it</ex></expan> <lb n="4"/><expan><abbr>an</abbr><ex>nis</ex></expan> XX <expan><abbr>P</abbr><ex>ublius</ex></expan> <expan><abbr>Ae</abbr><ex cert="low">lius </ex></expan> G <lb n="5"/>5 ula patro <lb n="6"/>nus <expan><abbr>liber</abbr><ex>tae</ex></expan> <lb n="7"/>libertae <expan><abbr>b</abbr><ex>ene</ex></expan> <lb n="8"/><expan><abbr>m</abbr><ex>erenti</ex></expan>.
                        
                        
