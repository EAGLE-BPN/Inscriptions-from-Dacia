    <ab>
                        <lb n="1"/>
                        
                        
                            
                            [- - - <lb n="2"/>pro <expan><abbr>sal</abbr><ex>ute</ex></expan> <expan><abbr>Impp</abbr><ex>eratorum</ex></expan>] <lb n="3"/>Sevẹ<supplied reason="lost">ri et</supplied> <lb n="4"/>Antọ<supplied reason="lost">nini</supplied> <lb n="5"/><expan><abbr>Aug<am>g</am></abbr><ex>ustorum</ex></expan> <supplied reason="lost"><expan><abbr>nn</abbr><ex>ostrorum</ex></expan></supplied>.
                        
                        
                    </ab>

                
                
