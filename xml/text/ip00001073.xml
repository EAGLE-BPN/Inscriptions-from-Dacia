                        <lb n="1"/>
                        
                        
                            
                            <expan><abbr>Co</abbr><ex>hortis</ex></expan> I <expan><abbr>Fl</abbr><ex>aviae</ex></expan> <expan><abbr>His</abbr><ex>panorum</ex></expan> <expan><abbr>m</abbr><ex>illiariae</ex></expan> <lb n="2"/><supplied reason="lost">(centuriae) V</supplied>ạ<expan><abbr>le</abbr><ex>rii</ex></expan> Eumeṇ<supplied reason="lost">is</supplied> <lb n="3"/><expan><abbr>n<am>n</am></abbr><ex>i</ex></expan> Ur <lb n="4"/>sionis.
                        
                        
