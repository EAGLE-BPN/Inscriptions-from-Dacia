                            <expan><abbr>I</abbr><ex>ovi</ex></expan> <expan><abbr>o</abbr><ex>ptimo</ex></expan> <expan><abbr>m</abbr><ex>aximo</ex></expan> <lb n="2"/><expan><abbr>Aurell</abbr><ex>ius</ex></expan> <lb n="3"/>Vitell <lb n="4"/>ianus <lb n="5"/><expan><abbr>sesq</abbr><ex>uiplicarius</ex></expan> <expan><abbr>p</abbr><ex>ro</ex></expan> <supplied reason="lost"> <expan><abbr>s</abbr><ex>e</ex></expan></supplied> <lb n="6"/>et <expan><abbr>sor</abbr><ex>ore</ex></expan> <expan><abbr>s</abbr><ex>ua</ex></expan> <lb n="7"/><expan><abbr>v</abbr><ex>otum</ex></expan> <expan><abbr>s</abbr><ex>olvit</ex></expan> <expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>m</abbr><ex>erito</ex></expan>.
                        
                        
