                            Deo Forti <lb n="2"/>Phoebo <lb n="3"/>Apollini <lb n="4"/>Parthico <lb n="5"/><expan><abbr>C</abbr><ex>aius</ex></expan> Cassius <lb n="6"/><expan><abbr>Vitalis</abbr><ex>centurio</ex></expan> <lb n="7"/><expan><abbr>l</abbr><ex>egionis</ex></expan> V <expan><abbr>M</abbr><ex>acedonicae</ex></expan> <expan><abbr>p</abbr><ex>iae</ex></expan> <expan><abbr>c</abbr><ex>onstantis</ex></expan> <lb n="8"/><expan><abbr>l</abbr><ex>ibens</ex></expan> <expan><abbr>pos</abbr><ex>uit</ex></expan>.
                        
                        
