                            Herculi <lb n="2"/>invicto <lb n="3"/>pro <expan><abbr>sal</abbr><ex>ute</ex></expan> <expan><abbr>M</abbr><ex>arci</ex></expan> <lb n="4"/><supplied reason="lost"> A</supplied><expan><abbr>ur</abbr><ex>elii</ex></expan> Ant <supplied reason="lost"> o</supplied> <lb n="5"/><supplied reason="lost"> nini <expan><abbr>Aug</abbr><ex>usti</ex></expan></supplied> <lb n="6"/><supplied reason="lost"> <gap reason="lost" quantity="1" unit="character"/> <gap reason="lost" quantity="1" unit="character"/> <gap reason="lost" quantity="1" unit="character"/> <gap reason="lost" quantity="1" unit="character"/> <gap reason="lost" quantity="1" unit="character"/> <gap reason="lost" quantity="1" unit="character"/></supplied>.
                        
                        
                    </ab>

                
                
