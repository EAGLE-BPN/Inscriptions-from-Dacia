    <ab>
                        <lb n="1"/>
                        
                        
                            
                            [- - - - - - <lb n="2"/>- - - <expan><abbr>vix</abbr><ex>it</ex></expan> <lb n="3"/><expan><abbr>an</abbr><ex>nis</ex></expan> ..<certainty match="preceding-sibling::node()" locus="value"/>]III <expan><abbr>Ael</abbr><ex>ius</ex></expan> [- - - <lb n="4"/>ve]ṭ(eranus) ex op[<expan><abbr>t</abbr><ex>ione</ex></expan> <lb n="5"/>..]<expan><abbr>P</abbr><ex cert="low"/></expan> et A[- - - <lb n="6"/>- - - - - -].
                        
                        
                    </ab>

                
                
