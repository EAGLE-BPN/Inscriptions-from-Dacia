    <ab>
                        <lb n="1"/>
                        
                        
                            
                            <supplied reason="lost" cert="low">- - - </supplied> <lb n="2"/>Bebeia Ty<expan><ex>c</ex><abbr>h</abbr></expan><supplied reason="lost">e</supplied> <lb n="3"/>ṇis <expan><abbr>vix</abbr><ex>it</ex></expan> <expan><abbr>an</abbr><ex>nis</ex></expan> <supplied reason="lost">..</supplied> <lb n="4"/><expan><abbr><supplied reason="lost">A</supplied>el</abbr><ex>ius</ex></expan> M<surplus>a</surplus>enena <lb n="5"/>chus (centurio) <expan><abbr>con</abbr><ex>iugi</ex></expan> <expan><abbr>b</abbr><ex>ene</ex></expan> <supplied reason="lost"><expan><abbr>m</abbr><ex>erenti</ex></expan></supplied> <lb n="6"/>5 <expan><abbr>pos</abbr><ex>uit</ex></expan>.
                        
                        
                    </ab>

                
                
